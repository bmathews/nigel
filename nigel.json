{
    "plugins": {
        "confluence": {
            "plugin": "confluence",
            "config": {
                "url": "https://confluence.atsid.com/",
                "auth": "auth.json"
            }
        },
        "cloudbees": {
            "plugin": "cloudbees",
            "config": {
                "url": "https://api.cloudbees.com/api",
                "auth": "auth.json"
            }
        },
        "directory": {
            "plugin": "confluence-directory",
            "config": {
                "url": "https://confluence.atsid.com/",
                "auth": "auth.json"
            }
        }
    },
    "projects": {
        "bti": {
            "displayName": "BTI Events",
            "description": "cool app",
            "plugins": {
                "build": {
                    "plugin": "jenkins",
                    "config": {
                        "url": "https://atsid.ci.cloudbees.com/",
                        "project": "bti-web-integ"
                    }
                },
                "doc": {
                    "plugin": "confluence",
                    "config": {
                        "url": "https://confluence.atsid.com/",
                        "spaceKey": "BE"
                    }
                },
                "bugs": {
                    "plugin": "jira",
                    "config": {
                        "project": "be",
                        "host": "jira.atsid.com",
                        "port": "443",
                        "auth": "auth.json"
                    }
                }
            }
        }
    }
}