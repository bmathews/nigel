{
    "protocols": {
        "http": {
            "path": "http"
        },
        "stdin": {
            "path": "stdin"
        }
    },
    "plugins": {
        "help": {
            "path": "help"
        },
        "confluence": {
            "path": "confluence",
            "config": {
                "url": "https://confluence.atsid.com/"
            },
            "auth": { "type": "userPass" }
        },
        "cloudbees": {
            "path": "cloudbees",
            "config": {
                "url": "https://api.cloudbees.com/api"
            },
            "auth": { "type": "cloudbees" }
        },
        "echo": {
            "path": "echo"
        },
        "rps": {
            "path": "rps"
        },
        "quote": {
            "path": "rss",
            "config": {
                "url": "feed://feeds.feedburner.com/brainyquote/QUOTEFU"
            }
        },
        "jcl": {
            "path": "jcl"
        },
        "contacts": {
            "path": "confluence-csv",
            "config": {
                "url": "https://confluence.atsid.com/download/attachments/8290557/ATS%20Contacts%201.18.13.xlsx"
            },
            "auth": { "type": "userPass" }
        },
        "lunch": {
            "path": "lunch",
            "config": {
                "lunches": [
                    "Subway",
                    "Chungs",
                    "Taco Time",
                    "Taco Bell",
                    "Arbys"
                ]
            }
        }
    },
    "projects": {
        "bti": {
            "displayName": "BTI Events",
            "description": "cool app",
            "plugins": {
                "doc": {
                    "path": "confluence",
                    "config": {
                        "url": "https://confluence.atsid.com/",
                        "spaceKey": "BE"
                    },
                    "auth": { "type": "userPass" }
                }
            }
        }
    }
}